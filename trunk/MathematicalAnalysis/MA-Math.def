\def\@empheqdef#1#2{%
  \setkeys{EmphEqEnv}{#1}%
  \setkeys{EmphEqOpt}{#2}%
  \EmphEqMainEnv}

\def\EMPHEQDEF#1#2{%
  \newenvironment{#1}[1][]{%
    \@empheqdef{#2}{##1}}{
    \endEmphEqMainEnv}}

